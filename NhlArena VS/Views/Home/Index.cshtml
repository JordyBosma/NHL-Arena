@{
    Layout = "_DefaultLayout";
    ViewData["Title"] = "Home Page";
}

@section Stylesheets{
    <link rel="stylesheet" href="~/css/Index.css" />
}
    <div class="container row index__container">
        <div class="col s6 collum__image">
            <div class="image__container">
                <img class="Index__NHLArenaLogo" src="~/images/NHLArenaLogo.png" alt="NHLArenaLogo" />
            </div>
        </div>
        <div class="index__homeinfo col s6">
            <div class="index__fillspace">
                <a class="btn" href="/Home/TestScene">TestWorldLink</a>
            </div>
            <div class="row">
                <div class="col s12 m10 offset-m1">
                    <div class="card red darken-4">
                        <div class="card-image">
                            <img src="~/images/stock.jpg" alt="Alternate Text" style="min-height: 150px; margin-top: 5px" />
                            <span class="card-title orange-text text-accent-3">Card Title</span>
                        </div>
                        <div class="card-content">
                            <p>
                                I am a very simple card. I am good at containing small bits of information.
                                I am convenient because I require little markup to use effectively.
                            </p>
                        </div>
                        <div class="card-action">
                            <a class="waves-effect waves-light btn orange accent-3" href="#">Join</a>
                            <a class="waves-effect waves-light btn orange accent-3" href="#">Login</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<script>
    var Socket = new WebSocket("ws://" + window.location.hostname + ":" + window.location.port + "/connect_client");
    Socket.onopen = function () {
